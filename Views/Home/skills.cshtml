
@{
    ViewBag.Title = "Skills";
}
<div class="masthead text-center">
    <div class="container h-100 pt-3">
        <img class="img-thumbnail pt-4" src="~/img/skills.jpg">
        <h1>Habilidades e Qualificações</h1>
    </div>

</div>

<div>
    <h2>
        <li> Idiomas:</li>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 60%; color:black; background-color:cornflowerblue" aria-valuenow="25" aria-valuemax="100">Inglês - 60%</div>
        </div>

        <li>Formação:</li>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 100%; color:black; background-color:#e08d8d" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Ciencias Contábeis - 100%</div>
        </div>

        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 85%; color:black; background-color:cornflowerblue" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Analise e Desenvolvimento de Sistemas - 85% </div>
        </div>

        <li>Conhecimentos:</li>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 40%; color:black; background-color:#e08d8d" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100" >HTML5 e CSS3 - 40%</div>
        </div>

        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 10%; color:black; background-color:cornflowerblue" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Java - 10% </div>
        </div>

        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 20%; color:black; background-color:#e08d8d" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">C# - 20%</div>
        </div>

        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 70%; color:black; background-color:cornflowerblue" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Scrum - 70% </div>
        </div>

        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 75%; color:black; background-color:#e08d8d" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Lógica - 75% </div>
        </div>

        <li>Curiosidades:</li>
        
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 40%; color:black; background-color:cornflowerblue" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">Fotografia - 40% </div>
        </div>

      

    </h2>
</div>







